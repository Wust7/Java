<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wust.online_class.dao.VideoMapper">
<!--    id:需要唯一-->
    <select id="selectById" resultType="com.wust.online_class.domain.Video">
        select * from video where id=#{video_id}
    </select>

    <select id="selectListByXML" resultType="com.wust.online_class.domain.Video">
        select * from video
    </select>

</mapper>